fun not X
  return if X then false else true
end

fun onStart []
  println "Game started"
end

fun check [Status, Message]
  if not Status then
    println ["Command failed:", Message]
  end
end

fun onTurn []
  println "My turn"
  println ["Market:", marketState []]
  [RawCount, RawPrice, ProdCount, ProdPrice] = marketState []
  check buy [2, RawPrice]
  check sell [2, ProdPrice]
  check produce 2
end
